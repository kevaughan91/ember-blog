


<div class="container">
    <div class="fashion--title">Shop my favorite styles!</div>
    <div class="fashion--subtitle">Click an image to view details on the outfit + accessories.</div>
    {{outlet}}
    <div class="row fashion--image-list-container">
        {{#each model as |fashion|}}
            <div class="col-md-3">{{#link-to "fashion.view" fashion.id}}<img class="fashion--image" src="/assets/images/{{fashion.image}}">{{/link-to}}</div>
        {{/each}}
    </div>
</div>