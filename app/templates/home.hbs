{{outlet}}
<div class="container">
    <div class="row">
        {{#each model as |postID|}}
            <div class="col-md-3 blog-post">
                {{#link-to "post" postID}}
                    <div class="overlay"></div>
                    <img class="blog-image" src="/assets/images/post{{postID}}.jpg">
                {{/link-to}}
            </div>
        {{/each}}

       
    </div>
</div>